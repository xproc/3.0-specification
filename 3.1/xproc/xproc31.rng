      <ref name="common.attributes"/>
      <ref name="global.attributes"/>
      <ref name="step.attributes"/>
      <group>
        <interleave>
          <optional>
            <ref name="WithInput"/>
          </optional>
          <optional>
            <ref name="Output"/>
          </optional>
          <zeroOrMore>
            <ref name="Documentation"/>
          </zeroOrMore>
          <zeroOrMore>
            <ref name="PipeInfo"/>
